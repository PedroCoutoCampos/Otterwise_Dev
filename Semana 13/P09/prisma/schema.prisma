generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model carro {
  id        Int    @id @default(autoincrement())
  name      String @db.VarChar(50)
  year      String @db.VarChar(50)
  marca_id  Int
  image_url String
}

model marca {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(50)
}
